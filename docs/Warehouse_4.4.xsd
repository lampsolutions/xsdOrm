<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<!-- Warehouse_basetypes starts here -->
	<xs:simpleType name="t_vstring60">
		<xs:annotation>
			<xs:documentation>string with variable length, 1 to 60 characters</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="60"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="t_vstring50">
		<xs:annotation>
			<xs:documentation>string with variable length, 1 to 50 characters</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="50"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="t_vstring120">
		<xs:annotation>
			<xs:documentation>string with variable length, 1 to 120 characters</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="120"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="t_vstring256">
		<xs:annotation>
			<xs:documentation>string with variable length, 1 to 256 characters</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="256"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="t_vstring1024">
		<xs:annotation>
			<xs:documentation>string with variable length, 1 to 1024 characters</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="1024"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="t_vstring2510">
		<xs:annotation>
			<xs:documentation>string with variable length, 1 to 2510 characters</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="2510"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="t_URI120">
		<xs:annotation>
			<xs:documentation>URI (Uniform Resource Identifier), 1 to 120 characters</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:anyURI">
			<xs:minLength value="1"/>
			<xs:maxLength value="120"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="t_price">
		<xs:annotation>
			<xs:documentation>Price</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:decimal"/>
	</xs:simpleType>
	<!-- Warehouse_types starts here -->
	<xs:simpleType name="t_fileURI">
		<xs:annotation>
			<xs:documentation>File location URI - up to 120 characters</xs:documentation>
		</xs:annotation>
		<xs:restriction base="t_URI120"/>
	</xs:simpleType>
	<xs:simpleType name="t_article_number">
		<xs:annotation>
			<xs:documentation>Article number - unique per supplier</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<xs:simpleType name="t_ean_upc">
		<xs:annotation>
			<xs:documentation>EAN (European Articel Number) or UPC (US Product Code) - 12 or 13 numerical digits</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:NMTOKEN">
			<xs:pattern value="[0-9]{12,13}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="t_grid">
		<xs:annotation>
			<xs:documentation>Global Release Identifier - 18 alphanumerical digits</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:NMTOKEN">
			<xs:pattern value="[0-9A-Za-z]{18}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="t_isrc">
		<xs:annotation>
			<xs:documentation>International Standard Recording Code</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<xs:simpleType name="t_pn_track_id">
		<xs:annotation>
			<xs:documentation>Phononet track ID</xs:documentation>
		</xs:annotation>
		<xs:restriction base="t_vstring50"/>
	</xs:simpleType>
	<xs:simpleType name="t_mailboxno">
		<xs:annotation>
			<xs:documentation>Phononet customer number - 4 numerical digits</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:NMTOKEN">
			<xs:pattern value="[0-9]{4}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="t_publisher">
		<xs:annotation>
			<xs:documentation>Name of publisher</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<xs:simpleType name="t_composer">
		<xs:annotation>
			<xs:documentation>Name of composer</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<xs:simpleType name="t_configuration">
		<xs:annotation>
			<xs:documentation>Configuration (product type)</xs:documentation>
		</xs:annotation>
		<xs:restriction base="t_vstring256"/>
	</xs:simpleType>
	<xs:simpleType name="t_dvd_regional_code">
		<xs:annotation>
			<xs:documentation>DVD regional code</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:NMTOKEN">
			<xs:enumeration value="00"/>
			<xs:enumeration value="01"/>
			<xs:enumeration value="02"/>
			<xs:enumeration value="03"/>
			<xs:enumeration value="04"/>
			<xs:enumeration value="05"/>
			<xs:enumeration value="06"/>
			<xs:enumeration value="A1"/>
			<xs:enumeration value="B2"/>
			<xs:enumeration value="C3"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="t_age_recommendation">
		<xs:annotation>
			<xs:documentation>Age recommendation</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<xs:simpleType name="t_price_category">
		<xs:annotation>
			<xs:documentation>Price category: high, medium, budget</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<xs:simpleType name="t_price_code">
		<xs:annotation>
			<xs:documentation>Price code - 4 alphanumerical characters</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<xs:simpleType name="t_charity_donation">
		<xs:annotation>
			<xs:documentation>Charity donation included in price - value in percent</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:decimal"/>
	</xs:simpleType>
	<xs:simpleType name="t_sales_area">
		<xs:annotation>
			<xs:documentation>Subdistribution code</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<xs:simpleType name="t_marketing_unit">
		<xs:annotation>
			<xs:documentation>Marketing unit</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<xs:simpleType name="t_person">
		<xs:annotation>
			<xs:documentation>Person's name</xs:documentation>
		</xs:annotation>
		<xs:restriction base="t_vstring256"/>
	</xs:simpleType>
	<xs:simpleType name="t_label_name">
		<xs:annotation>
			<xs:documentation>Label name</xs:documentation>
		</xs:annotation>
		<xs:restriction base="t_vstring120"/>
	</xs:simpleType>
	<xs:simpleType name="t_label_short_name">
		<xs:annotation>
			<xs:documentation>Label's short name</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="10"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="t_label_code">
		<xs:annotation>
			<xs:documentation>Label code</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:NMTOKEN">
			<xs:pattern value="[0-9]{5}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="t_artist_name">
		<xs:annotation>
			<xs:documentation>Artist name</xs:documentation>
		</xs:annotation>
		<xs:restriction base="t_vstring256"/>
	</xs:simpleType>
	<xs:simpleType name="t_title">
		<xs:annotation>
			<xs:documentation>Title, free text up to 120 characters</xs:documentation>
		</xs:annotation>
		<xs:restriction base="t_vstring256"/>
	</xs:simpleType>
	<xs:simpleType name="t_title_version">
		<xs:annotation>
			<xs:documentation>Title version</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:positiveInteger">
			<xs:maxInclusive value="99"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="t_trackno">
		<xs:annotation>
			<xs:documentation>Sequential track number</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:unsignedInt"/>
	</xs:simpleType>
	<xs:simpleType name="t_setno">
		<xs:annotation>
			<xs:documentation>Sequential set number if product consists of multiple media</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:positiveInteger">
			<xs:maxInclusive value="999"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="t_playtime">
		<xs:annotation>
			<xs:documentation>Playtime in mmm:ss</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:normalizedString">
			<xs:pattern value="[0-9]{1,6}:[0-9]{2}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="t_description">
		<xs:annotation>
			<xs:documentation>Description</xs:documentation>
		</xs:annotation>
		<xs:restriction base="t_vstring1024"/>
	</xs:simpleType>
	<xs:simpleType name="t_comment">
		<xs:annotation>
			<xs:documentation>Comment</xs:documentation>
		</xs:annotation>
		<xs:restriction base="t_vstring1024"/>
	</xs:simpleType>
	<xs:simpleType name="t_genre_code">
		<xs:annotation>
			<xs:documentation>Genre (code) - see Phononet codelist</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:unsignedInt"/>
	</xs:simpleType>
	<xs:simpleType name="t_genre">
		<xs:annotation>
			<xs:documentation>Genre name</xs:documentation>
		</xs:annotation>
		<xs:restriction base="t_vstring120"/>
	</xs:simpleType>
	<xs:simpleType name="t_keyword">
		<xs:annotation>
			<xs:documentation>Keyword (code) - see Phononet codelist</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:NMTOKEN">
			<xs:minLength value="1"/>
			<xs:maxLength value="10"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="t_picture_format">
		<xs:annotation>
			<xs:documentation>Picture file format</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:NMTOKEN">
			<xs:enumeration value="jpg"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="t_picture_resolution">
		<xs:annotation>
			<xs:documentation>Picture resolution in pixel</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:positiveInteger"/>
	</xs:simpleType>
	<xs:simpleType name="t_picture_title">
		<xs:annotation>
			<xs:documentation>Picture title, up to 60 characters</xs:documentation>
		</xs:annotation>
		<xs:restriction base="t_vstring60"/>
	</xs:simpleType>
	<xs:simpleType name="t_artwork_type">
		<xs:annotation>
			<xs:documentation>Artwork's type</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:NMTOKEN">
			<xs:enumeration value="front"/>
			<xs:enumeration value="back"/>
			<xs:enumeration value="extra"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="t_artwork_pageno">
		<xs:annotation>
			<xs:documentation>Artwork's page number</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:positiveInteger">
			<xs:maxInclusive value="99"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="t_sound_type">
		<xs:annotation>
			<xs:documentation>Sound type - 'prelistening' or 'download'</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:NMTOKEN">
			<xs:enumeration value="prelistening"/>
			<xs:enumeration value="download"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="t_codec">
		<xs:annotation>
			<xs:documentation>Media Codec: wav, real, wma, mp3, flv, webm, mp4, ogg</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:NMTOKEN">
			<xs:enumeration value="wav"/>
			<xs:enumeration value="real"/>
			<xs:enumeration value="wma"/>
			<xs:enumeration value="mp3"/>
			<xs:enumeration value="flv"/>
			<xs:enumeration value="webm"/>
			<xs:enumeration value="mp4"/>
			<xs:enumeration value="ogg"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="t_bitrate">
		<xs:annotation>
			<xs:documentation>Track's encoding bitrate in kbps</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:positiveInteger"/>
	</xs:simpleType>
	<xs:simpleType name="t_channel">
		<xs:annotation>
			<xs:documentation>Track's encoding channels: mono, stereo, 5:1</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<xs:simpleType name="t_sampling_rate">
		<xs:annotation>
			<xs:documentation>Track's sampling rate in Hz</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:positiveInteger"/>
	</xs:simpleType>
	<!-- Warehouse_types ends here -->
	<xs:element name="file" type="t_fileURI">
		<xs:annotation>
			<xs:documentation>Filename</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="t_picture">
		<xs:annotation>
			<xs:documentation>Picture</xs:documentation>
		</xs:annotation>
		<xs:sequence maxOccurs="unbounded">
			<xs:element name="resolution_x">
				<xs:annotation>
					<xs:documentation>Picture resolution, x-axis</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="t_picture_resolution"/>
				</xs:simpleType>
			</xs:element>
			<xs:element name="resolution_y" type="t_picture_resolution">
				<xs:annotation>
					<xs:documentation>Picture resolution, y-axis</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="format" type="t_picture_format">
				<xs:annotation>
					<xs:documentation>Picture file format</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="file">
				<xs:annotation>
					<xs:documentation>Picture file location</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="md5_checksum" type="t_vstring256">
				<xs:annotation>
					<xs:documentation>cover md5 checksum</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="link" type="t_vstring256" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Link to cover server</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="picture_text" type="t_picture_title" minOccurs="0"/>
			<xs:element name="copyright" type="t_vstring1024" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="t_artwork_picture">
		<xs:annotation>
			<xs:documentation>Artwork picture - extends type 'picture' by element 'title'</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="t_picture">
				<xs:attribute name="type" type="t_artwork_type" use="required">
					<xs:annotation>
						<xs:documentation>Artwork type - 'cover', 'front' or 'booklet'</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="page" type="t_artwork_pageno" use="required">
					<xs:annotation>
						<xs:documentation>Artwork page number - from 1 to 99</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="t_sound">
		<xs:annotation>
			<xs:documentation>Sound information</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="file">
				<xs:annotation>
					<xs:documentation>Picture resolution, x-axis</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="sound_type" type="t_sound_type">
				<xs:annotation>
					<xs:documentation>Sound type, here always prelistening</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="codec" type="t_codec">
				<xs:annotation>
					<xs:documentation>Soundfile format</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="bitrate" type="t_bitrate" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Soundfile bitrate in kbps</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="channels" type="t_channel" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Soundfile channel information</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="sampling_rate" type="t_sampling_rate" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Soundfile sampling rate in Hz</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="playtime" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:group name="g_age_recommendation">
		<xs:annotation>
			<xs:documentation>Age recommendation</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="FSK" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Film ratings, german FSK (Freiwillige Selbstkontrolle) 0020017001</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="recommended_age" type="t_age_recommendation" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Recommended age minimum 0020017002</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PEGI_recommended_age" type="t_age_recommendation" minOccurs="0">
				<xs:annotation>
					<xs:documentation>PEGI age recommendation 0020017051</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PEGI" type="xs:NMTOKEN" minOccurs="0" maxOccurs="8">
				<xs:annotation>
					<xs:documentation>PEGI warning 0020017052 - 0020017059, values: violence, bad_language, fear, drug_abuse, sex, discrimination, gambling, online</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:group>
	<xs:group name="g_product">
		<xs:annotation>
			<xs:documentation>Product information</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="configuration_number">
				<xs:annotation>
					<xs:documentation>Configuration number 0020010005</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="t_configuration">
						<xs:maxLength value="4"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="configuration" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Configuration (product type) text</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="artist">
				<xs:annotation>
					<xs:documentation>Main artist 0020010002</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="title" type="t_title">
				<xs:annotation>
					<xs:documentation>Title (50-characters, alphanumeric) 0020010001</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="composer" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Composer 0020010003</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="title_addition" type="t_vstring120" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Title addition 0020014001</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="dvd_regional_code" type="t_dvd_regional_code" minOccurs="0">
				<xs:annotation>
					<xs:documentation>DVD regional code (two digits) 002001006</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="actor" type="t_vstring120" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Actor (DVD movie) 0020010007</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="language" type="t_vstring120" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Languages supported (DVD movie) 0020010008</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="aspect_ratio" type="t_vstring50" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Movie aspect ratio, e.g. 16:9 (DVD movie) 0020010009</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="studio" type="t_vstring50" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Studio information (DVD movie) 0020010010</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="total_playtime" type="t_playtime" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Total playtime (DVD movie) 0020010011</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="features" type="t_vstring120" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Features (DVD movie) 0020010012</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="sound_format" type="t_vstring50" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Sound format (DVD movie) 0020010013</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="article_info" type="t_vstring1024" minOccurs="0">
				<xs:annotation>
					<xs:documentation>General product information 00200100014</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="subtitle" type="t_vstring50" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Subtitle information 0020010015</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="original_title" minOccurs="0">
				<xs:annotation>
